{
  "name": "react-orcus",
  "version": "0.0.0",
  "description": "React library for creating an app with a windowed desktop interface.",
  "dependencies": {
    "@material-ui/core": "^4.0.2",
    "extend": "^3.0.2",
    "memoize-one": "^5.0.4",
    "prop-types": "^15.7.2",
    "q": "^1.5.1",
    "re-resizable": "^5.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-draggable": "^3.3.0"
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.1",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "check-prop-types": "^1.1.2",
    "dev-tasks": "github:joshuacwebdeveloper/dev-tasks#16abf14",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-util": "^3.0.8",
    "jquery": "^3.4.1",
    "jsdom": "^15.1.1",
    "karma": "^4.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "mocha": "^6.1.4",
    "react-hyperscript": "^3.2.0",
    "requirejs": "^2.3.6",
    "sinon": "^7.3.2"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "main": "build/index.js",
  "scripts": {
    "gulp": "gulp",
    "lint": "gulp lint",
    "build": "gulp build",
    "bundle": "gulp bundle",
    "minify": "gulp minify",
    "release": "gulp release",
    "test": "(npm run test-mocha || true) && npm run test-karma",
    "test-inspect": "(npm run test-mocha-inspect || true) && npm run test-karma-inspect",
    "test-mocha": "mocha --require test/util/setup.util.js test/suites/*.test.js",
    "test-mocha-inspect": "mocha --inspect --debug-brk --require test/util/setup.util.js test/suites/*.test.js",
    "test-karma": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test-karma-inspect": "karma start --single-run=false --browsers ChromeHeadless karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JoshuaCWebDeveloper/react-orcus"
  },
  "author": "Joshua Carter",
  "license": "MIT"
}
